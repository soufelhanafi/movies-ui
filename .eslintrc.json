{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parser": "babel-eslint",
  "plugins": ["react"],
  "rules": {
    "no-console": 1,
    "no-debugger": 1,
    "no-undef": 1,
    "no-unused-vars": 1,
    "react/prop-types": 0,
    "react/display-name": 0,
    "no-underscore-dangle": 0,
    "no-unreachable": 0,
    "react/no-unescaped-entities": 0,
    "no-case-declarations": 0,
    "no-empty": 1,
    "react/jsx-no-duplicate-props": 1,
    "expected_a_at_b_c": 0,
    "react/jsx-no-comment-textnodes": 1,
    "no-redeclare": 1,
    "no-mixed-spaces-and-tabs": 0,
    "react/no-deprecated": 0,
    "react/no-string-refs": 0,
    "react/no-find-dom-node": 0,
    "no-prototype-builtins": 0
  },
  "globals": {
    "window": "writable",
    "document": "writable",
    "localStorage": "writable",
    "FormData": "writable",
    "FileReader": "writable",
    "Blob": "writable",
    "navigator": "writable",
    "Promise": "writable",
    "Intl": "writable",
    "setTimeout": "writable",
    "process": "writable",
    "global": "writable",
    "File": "writable",
    "places": "writable",
    "setInterval": "writable",
    "module": "writable",
    "describe": "writable",
    "fetch": "writable",
    "clearInterval": "writable",
    "caches": "writable",
    "Chart": "readable"
  },
  "env": {
    "jest": true,
    "es6": true,
    "browser": true,
    "node": true
  }
}
